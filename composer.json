{
    "name":        "xannn94/localization",
    "description": "Localization Package for Laravel",
    "keywords":    ["xannn94", "laravel", "localization", "localisation"],
    "authors": [
        {
            "name":     "Xannn94",
            "email":    "axiles94@gmail.com",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "version": "1.0.0",
    "require": {
        "php" :              ">=5.6",
        "xannn94/support": "1.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpcov":    "~2.0|~3.0",
        "phpunit/phpunit":   "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Xannn94\\Localization\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Xannn94\\Localization\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
    "suggest": {
        "ext-intl": "Use Intl extension for 'Locale' class (an identifier used to get language)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
